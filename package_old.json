{
  "name": "back-modulejs",
  "version": "0.0.1",
  "description": "Node.js API with Koa-Smart, Sequelize & Postgresql",
  "author": "epitech",
  "license": "ISC",
  "engines": {
    "node": "16.13.0",
    "npm": "8.1.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start": "npm run dev",
    "prod": "node dist/index.js",
    "_rm-build-prod": "rimraf dist",
    "_build-prod": "cross-env NODE_ENV=production babel src --out-dir dist",
    "build-prod": "npm run _rm-build-prod && npm run _build-prod",
    "test": "cross-env NODE_ENV=test mocha --opts mocha.opts",
    "lint": "eslint src"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@prisma/client": "^3.3.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa-smart": "^3.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "expect": "^24.8.0",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.1",
    "prisma": "^3.3.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "apidoc"
    ]
  }
}